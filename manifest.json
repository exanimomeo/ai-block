{
    "manifest_version": 3,
    "name": "AI Block",
    "version": "1.0",
    

    "description": "Blocks various AI-Generated content throughout the web.",
    "icons":{
        "48": "icons/border-48.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["ai-block.js"],
            "run_at": "document_idle"
        }
    ]
}